<?xml version="1.0" encoding="utf-8"?>
<com.qamaster.android.ui.TestCycleAdapterRow xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="48dp"
    android:background="@drawable/qamaster_user_list_background"
    android:orientation="horizontal"
    android:paddingBottom="0dp"
    android:paddingTop="0dp" >

    <TextView
        android:id="@+id/qamaster_test_cycle_name"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:duplicateParentState="true"
        android:focusable="false"
        android:gravity="left|center_vertical"
        android:paddingLeft="16dp"
        android:textColor="@drawable/qamaster_user_list_textcolor"
        android:textSize="16sp"
        android:textStyle="normal"
        tools:text="Test Cycle text" />

</com.qamaster.android.ui.TestCycleAdapterRow>